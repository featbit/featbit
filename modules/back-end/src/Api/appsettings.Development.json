{
  "Logging": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Information",
        "Npgsql.Command": "Warning",
        "Infrastructure.MQ": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Jwt": {
    "Issuer": "featbit",
    "Audience": "featbit-api",
    "Key": "featbit-identity-key-must-longer-than-32-characters"
  },
  "DbProvider": "MongoDb",
  "MqProvider": "Redis",
  "CacheProvider": "Redis",
  "MongoDb": {
    "ConnectionString": "mongodb://admin:password@localhost:27017",
    "Database": "featbit"
  },
  "Postgres": {
    "ConnectionString": "Host=localhost;Port=5432;Username=postgres;Password=0tJXCokSvOB8;Database=featbit"
  },
  "Kafka": {
    "Producer": {
      "bootstrap.servers": "localhost:29092",
      "linger.ms": "50"
    },
    "Consumer": {
      "group.id": "featbit-api",
      "bootstrap.servers": "localhost:29092",
      "auto.offset.reset": "earliest",
      "enable.auto.commit": true,
      "auto.commit.interval.ms": "5000",
      "enable.auto.offset.store": false
    }
  },
  "OLAP": {
    "ServiceHost": "http://localhost:8200"
