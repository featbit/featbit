{
  "name": "featbit",
  "version": "2.0.0",
  "scripts": {
    "start": "npm run start:en",
    "start:en": "ng serve --configuration=development --port=4200",
    "start:zh": "ng serve --configuration=development,zh --port=4201",
    "build:dev": "ng build --localize --configuration development",
    "build:prod": "ng build --localize --configuration production",
    "i18n": "ng extract-i18n --output-path src/locale && locales-generator --l zh --bp ./src/locale && bash ./change_validations.sh",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage"
  },
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@antv/g2": "^4.2.10",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "0.39.0",
    "ng-zorro-antd": "16.0.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular/cli": "~16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@angular/localize": "^16.1.3",
    "@types/jasmine": "~4.3.5",
    "@types/karma-coverage": "^2.0.1",
    "angular-locales-generator": "^1.0.4",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.6"
  }
}
